<div class="a-rating">
    <ul class="a-rating__list">
        {{#each rating.items}}
        <li class="a-rating-item">
            <div class="a-rating-item__icon {{#if isActive}}is-active{{/if}}">
                {{> common/atoms/svg-icon
                iconId="icon-globe"
                iconWidth="18"
                iconHeight="18"
                }}
            </div>
        </li>
        {{/each}}
    </ul>
</div>